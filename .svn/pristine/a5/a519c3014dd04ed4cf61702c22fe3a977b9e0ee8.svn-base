package kr.or.dduk.ams.service;

import java.util.List;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

import kr.or.dduk.mapper.AdmissionMapper;
import kr.or.dduk.vo.PatientVO;

@Service
public class AdmissionServiceImpl implements AdmissionService{

	@Autowired
	private AdmissionMapper admissionMapper;
	
	//입원대기 환자 조회
	@Override
	public List<PatientVO> selectAdmissionList() {
		return admissionMapper.selectAdmissionList();
	}

	//퇴원대기 환자 조회
	@Override
	public List<PatientVO> selectDischargeList() {
		return admissionMapper.selectDischargeList();
	}
	

}
